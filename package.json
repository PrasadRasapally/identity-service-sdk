{
  "name": "identity-service-angularjs-sdk",
  "description": "An AngularJS Module implementing common use cases encountered when integrating AngularJS apps with the precorconnect identity service.",
  "homepage": "https://bitbucket.org/precorconnect/identity-service-angularjs-sdk",
  "private": true,
  "version": "0.0.58",
  "repository": {
    "type": "git",
    "url": "bitbucket:precorconnect/identity-service-angularjs-sdk"
  },
  "devDependencies": {
    "jspm": "^0.16.0",
    "jspm-git": "^0.3.3"
  },
  "directories": {
    "lib": "src"
  },
  "main": "module",
  "scripts": {
    "version": "git add -A",
    "postversion": "git push && git push --tags"
  },
  "jspm": {
    "format": "es6",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "aurelia-dependency-injection": "github:aurelia/dependency-injection@^0.9.2",
      "aurelia-fetch-client": "github:aurelia/fetch-client@^0.1.2",
      "fetch": "github:github/fetch@^0.9.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.22",
      "babel-runtime": "npm:babel-runtime@^5.8.20",
      "core-js": "npm:core-js@^1.1.0"
    }
  }
}
