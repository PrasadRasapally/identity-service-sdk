{
  "name": "identity-service-sdk",
  "description": "The precorconnect identity service javascript SDK.",
  "homepage": "https://bitbucket.org/precorconnect/identity-service-javascript-sdk",
  "private": true,
  "version": "0.0.72",
  "repository": {
    "type": "git",
    "url": "bitbucket:precorconnect/identity-service-javascript-sdk"
  },
  "devDependencies": {
    "jasmine": "^2.3.2",
    "jspm": "^0.16.0",
    "jspm-git": "^0.3.3",
    "karma": "^0.13.9",
    "karma-cli": "^0.1.0",
    "karma-jasmine": "^0.3.6",
    "karma-jspm": "^2.0.1",
    "karma-junit-reporter": "^0.3.3",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-spec-reporter": "0.0.20",
    "phantomjs": "^1.9.18"
  },
  "scripts": {
    "install": "jspm install",
    "test": "karma start",
    "preversion": "npm test",
